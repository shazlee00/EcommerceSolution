@model IEnumerable<Category>

<h1>Categories</h1>


<a asp-controller="Category" asp-action="Create">Create</a>

<table class="table table-hover">

	<thead>
		<th>Name</th>
		<th>Description</th>
		<th>Created Time</th>
		<th></th>
	</thead>
	<tbody>
	
		@foreach(var item in Model)
		{
			<tr>
				<td>@item.Name </td>
				<td>@item.Description</td>
				<td> @item.CreatedTime </td>
			</tr>
		}

	</tbody>



</table>