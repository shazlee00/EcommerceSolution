@model IEnumerable<ApplicationUser>
@{
	ViewBag.Title = "Customers";
	ViewBag.CardTitle = "users";
	Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}
<h1>Users</h1>



<partial name="~/Views/Shared/_ToasterPartial.cshtml" />



<a class="btn btn-primary" asp-area="Identity" asp-controller="Account" asp-action="Register">Create User</a>



<table class="table table-hover">

	<thead>
	<th>Name</th>
	<th>Email</th>
	<th>Adress</th>
	<th></th>
	</thead>
	<tbody>

		@foreach (var item in Model)
		{
			<tr>
				<td>@item.Name </td>
				<td>@item.Email </td>
				<td> @item.Adress </td>
				<td>
					<a class="btn btn-success" asp-controller="Users" asp-action="Lock" asp-route-id="@item.Id">
						<i class="fa fa-lock-open"></i>
					</a>
					<a class="btn btn-danger" asp-controller="Users" asp-action="Delete" asp-route-id="@item.Id">Delete</a>
				</td>
			</tr>
		}

	</tbody>



</table>